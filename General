Insert into etrade.CustomsSystemUserRequiredDocuments(CustomsBusinessActivityId,DocumentId,ServiceId) Values (null,332295721,68)


alter view [etrade].[CustomsSystemUserRequiredDocumentsVw]
AS
Select cba.CustomsBusinessActivityId,cba.DocumentId, Name as [EngName], LocalDescription  as [AraName] ,cba.ServiceId
From etrade.CustomsSystemUserRequiredDocuments cba
				inner join Types tt on tt.TypeId = cba.DocumentId



