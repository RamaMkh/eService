 var z = await _IeServicesUnitOfWork.CustomsSystemUserActivityRepository.Get()
.Where(p => p.CustomsSystemUserId == customsSystemUser.Id)
.GroupBy(p => p.CustomsBusinessActivityId)
.Select(p => p.OrderByDescending(g => g.DateCreated).First())
.Select(a => new CustomsBusinessActivities
{
    CustomsBusinessActivityId = a.CustomsBusinessActivityId,
    Status = _Language == LanguageEnum.English ? a.StatusEnglishName : a.StatusArabicName,
    ActivityName = _Language == LanguageEnum.English ? a.ActivityNameEnglish : a.ActivityNameArabic,
    Availability = a.Availability.Contains("Deleted") ? _Localizer[Resources.ResourcesEnum.SomethingWentWrong] : _Localizer[Resources.ResourcesEnum.Number],
    DateCreated = a.DateCreated
})
.ToListAsync();
