 public async Task<ServiceResponse> SubmitRenewLicense(OrgLicenceManagement requestDetails, ServiceRequest eserviceRequest, string userId= null, string language= null)
 {
     ServiceResponse response = new ServiceResponse();
     if(requestDetails.ServiceId == ((int)ServiceTypesEnum.ImportLicenseRenewalRequest).ToString())
     {
         ReqObj obj = new ReqObj()
         {
             CommonData = userId,
             CommonData1 = language,
             CommonData2 = ((int)ServiceTypesEnum.ImportLicenseRenewalRequest).ToString()
         };

         var formData = MobileDataBase.GetInitializeOrgLicenseRenewalRequest(obj);
     }
 
     var data = MobileDataBase.SubmitOrgLicenseRequest(requestDetails);

  

     if (data != null && data.Tables?.Count > 0 && data.Tables[0]?.Rows?.Count > 0)
     {
         int StatusCode =Convert.ToInt32( data.Tables[0].Rows[0]["Status"].ToString());
         if (StatusCode == 1)
         {
             response.Succeeded = true;
             response.Message = ResourcesEnum.RequestSubmittesSuccess;
         }
         else
         {
             response.Message = ResourcesEnum.ActiveRequestOrWorkingData;

         }
     }
     else
     {
         response.Message = ResourcesEnum.ErrorOccurred;
     }
     eserviceRequest.ServiceRequestsDetail.ReadyForSahelSubmission = false;
     _ieServicesUnitOfWork.ServiceRequestRepository.Update(eserviceRequest);
      _ieServicesUnitOfWork.Commit();

     return response;
 }
