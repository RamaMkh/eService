    string orgId = string.Empty;
    if(string.IsNullOrEmpty(userId))
    {
        return orgId;
    }
    var x = _IeServicesUnitOfWork.MobileUserOrgMapRepository
    .Join(_IeServicesUnitOfWork.UserRepository,
        a => new { a.UserId, a.ParentOrgTradeLicence },
        b => new { b.UserId, b.LicenseNumber },
        (a, b) => new { a, b })
    .Where(x => x.a.IsActive && x.a.UserId == userId)
    .OrderByDescending(x => x.a.MobileUserOrgMapId)
    .Select(x => x.a.OrganizationId)
    .FirstOrDefault();
}
