public async Task<string> GetLicenseValue(Int64 organizationId, int serviceId)
{
    string licenseNo = string.Empty;
    if (serviceId == (int)ServiceTypesEnum.ImportLicenseRenewalRequest)
    {
        licenseNo = _IeServicesUnitOfWork.OrganizationsRepository.Get(a => a.OrganizationId == organizationId)
            .Select(a=>a.ImporterLicenseNo).FirstOrDefault();

    }
    else if(serviceId == (int)ServiceTypesEnum.IndustrialLicenseRenewalRequest)
    {
        licenseNo = _IeServicesUnitOfWork.OrganizationsRepository.Get(a => a.OrganizationId == organizationId)
           .Select(a => a.IndustrialLicenseNo).FirstOrDefault();
    }
    else if (serviceId == (int)ServiceTypesEnum.CommercialLicenseRenewalRequest)
    {
        licenseNo = _IeServicesUnitOfWork.OrganizationsRepository.Get(a => a.OrganizationId == organizationId)
           .Select(a => a.CommercialLicenseNo).FirstOrDefault();
    }
    return licenseNo;
}
