
                                var checkCommecrialLic = await domainServices.CheckCommercialLic(_UserOrganizationId, LD.CommercialLicenceNumber);
                                if(checkCommecrialLic == CheckCommercialLicenseResultEnum.WrongOrganizationId)
                                {
                                    throw new BusinessRuleException(_Localizer[Resources.ResourcesEnum.SomethingWentWrong]);
                                }
                                else if (checkCommecrialLic == CheckCommercialLicenseResultEnum.CommercialLicNotRetreived)
                                {
                                    throw new BusinessRuleException(_Localizer[Resources.ResourcesEnum.CommercialLicNotRetreived]);
                                }
                                else if (checkCommecrialLic == CheckCommercialLicenseResultEnum.CommecrcialLicChanged)
                                {
                                    throw new BusinessRuleException(_Localizer[Resources.ResourcesEnum.CommecrcialLicChanged]);
                                }
