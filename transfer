
                       List<int> businessActivitiesList = await _IeServicesUnitOfWork.EServiceRequestsCustomsActivityRepository.Get()
                           .Where(x => x.EServiceRequestsId == serviceRequestDetails.EserviceRequestId)
                           .Select(x => x.CustomsBusinessUserActivityId)
                           .ToListAsync();

                        var activitesDetails = await _IeServicesUnitOfWork.OrganizationCustomsBusinessActivityRepository.Get()
                            .Where(a => a.OrganizationId == _UserOrganizationId)
                            .ToListAsync();
