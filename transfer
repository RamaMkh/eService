  using (var sCmd = new SqlCommand("[etrade].[sp_GetKMIDVerificationStatus]", sCon))
  {
      sCmd.CommandType = CommandType.StoredProcedure;
      sCmd.Parameters.Add("@csvTokens", SqlDbType.NVarChar).Value = sCSVTokens;

      DataSet Ds = new DataSet();
      SqlDataAdapter da = new SqlDataAdapter(sCmd);
      da.Fill(Ds);
      dtResult = Ds.Tables[0];
  }
