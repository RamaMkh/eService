 CREATE  TABLE #temp_orgactivity(activityid int) ;
 insert into #temp_orgactivity (activityid)
 select activityid FROM OrgUserActivities where personalid=1861128;
select * from  #temp_orgactivity 
DROP TABLE #temp_orgactivity;

SELECT *
FROM OrgUserActivities where personalid=1861104;
 select 

 ORGAct.ActivityId,
 BA.NameArabic as ActivityNameArabic,
 Ba.NameEnglish as ActivityNameEngkish,
 case
  when  ORGAct.ActivityId if exists (#temp_orgactivity) then 'Added'
  else 'Removed' as RequestStatus

 
 from [$OrgUserActivities] ORGAct 
 left join etrade.CustomsBusinessActivities_BPN BA on BA.Id = ORGAct.ActivityId
 where personalid=1861104 order by registereddate desc
