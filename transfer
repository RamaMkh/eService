 SahelTokensGenerator tokens = new SahelTokensGenerator();
 var createdTokens = await _ieServicesUnitOfWork.ServiceRequestsDetailRepository.Get(a => !string.IsNullOrEmpty(a.SahelToken1) && !string.IsNullOrEmpty(a.SahelToken2))
     .Select(a => new SahelTokensGenerator
     {
         Token1 = a.SahelToken1,
         Token2 = a.SahelToken2
     })
     .ToListAsync();
 while(true)
 {
     tokens.Token1 = eServicesV2.Kernel.Service.ServiceRequestServices.ServiceRequestService.fnGenerateToken();
     tokens.Token2 = eServicesV2.Kernel.Service.ServiceRequestServices.ServiceRequestService.fnGenerateToken();
     
     
 }
